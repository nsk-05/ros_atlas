<?xml version="1.0"?>
<launch>

  <!-- Load the URDF into ROS parameter server -->
  <param name="grid_description" command="$(find xacro)/xacro '$(find bot_gazebo)/urdf/full_grid.urdf.xacro'" />
 <!-- <param name="robot_description" command="$(find xacro)/xacro '$(find grid_description)/full_model/omni_bot.urdf.xacro' " /> -->
  <!-- <param name="camera_description" command="$(find xacro)/xacro '$(find grid_description)/urdf/camera.urdf.xacro' " /> -->

  <include file="$(find gazebo_ros)/launch/empty_world.launch" />
  <!-- Publish joint values -->
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />

  <!-- convert joint states to TF transforms for rviz, etc  -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>

  <!-- Spawn model -->
   <node name="grid_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
    args="-urdf -x 0 -y 0 -z 0  -model grid_base -param grid_description"/>
    
  <!--<node name="bot_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
	args="-urdf -x -8 -y -8 -z 0.3 -Y 1.5708 -model bot -param robot_description"/> -->
    

  <!-- <node name="camera_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
    args="-urdf -x 0 -y 0 -z 5.0  -model camera -param camera_description"/>-->

</launch>

